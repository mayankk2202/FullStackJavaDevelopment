
public class PrintArray {

	public void printArray(int [] array1) {
		System.out.println("Array values:");
		for(int i=0; i<array1.length;i++) {
			System.out.println("V"+i+" = "+array1[i]);
		}
	}
		public void printArray(float [] array1) {
			System.out.println("Array values:");
			for(int i=0; i<array1.length;i++) {
				System.out.println("V"+i+" = "+array1[i]);
			}
	}
}
